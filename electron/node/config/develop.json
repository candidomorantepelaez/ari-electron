{
  "app": {
    "profile": "development",
    "basePath": "http://localhost:8080",
    "frontUrl": "http://localhost:3030",
    "apiVersion": "/api/v1",
    "electron": true
  },
  "actions": {
    "type": "basic"
  },
  "auth": {
    "type": "jsonwebtoken",
    "secretkey": "secretkey"
  },
  "roles": {
    "client": "CLIENT_ROLE",
    "seller": "SELLER_ROLE",
    "admin": "ADMIN_ROLE"
  },
  "logger": {
    "type": "winston",
    "pathLog": "/.ariLog/combined.log",
    "pathError": "/.ariLog/error.log"
  },
  "cookies": {
    "domain": "ari",
    "path": "/",
    "url": "http://localhost",
    "name": "session-id"
  },
  "http": {
    "cors": true,
    "AccessControlAllowOrigin": "*",
    "AccessControlAllowHeaders": "Authorization, X-API-KEY, Origin, X-Requested-With, Content-Type, Accept, Access-Control-Allow-Request-Method",
    "AccessControlAllowMethods": "GET, POST, OPTIONS, PUT, DELETE",
    "Allow": "GET, POST, OPTIONS, PUT, DELETE"
  },
  "persistent": {
    "type": "nedb",
    "pathBbdd": "/.ariDb"
  }
}